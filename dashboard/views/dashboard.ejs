<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Insurance Dashboard</title>
  <link rel="stylesheet" href="/css/style.css">
</head>
<body>
  <h1>Insurance Applications Dashboard</h1>
  
  <table>
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Date of Birth</th>
        <th>Gender</th>
        <th>Contact Number</th>
        <th>Email</th>
        <th>Insurance Type</th>
        <th>Coverage Amount</th>
        <th>Policy Start Date</th>
        <th>Comments</th>
      </tr>
    </thead>
    <tbody>
      <% applications.forEach(function(application) { %>
        <tr>
          <td><%= application.first_name %></td>
          <td><%= application.last_name %></td>
          <td><%= application.dob %></td>
          <td><%= application.gender %></td>
          <td><%= application.contact_number %></td>
          <td><%= application.email %></td>
          <td><%= application.insurance_type %></td>
          <td><%= application.coverage_amount %></td>
          <td><%= application.policy_start_date %></td>
          <td><%= application.comments %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>

  <script src="/js/script.js"></script>
</body>
</html>
